{
    "extends": ["airbnb-base"],
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    },
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {
      "comma-dangle": [
        "error",
        {
          "arrays": "always-multiline",
          "objects": "always-multiline",
          "imports": "always-multiline",
          "exports": "always-multiline",
          "functions": "ignore"
        }
      ]
    }
  }